{
  "name": "Environment Notifier",
  "version": "2.0",
  "manifest_version": 3,
  "description": "Color-coded transparent banners for various Labflow environments on .com and .ca.",
  "content_scripts": [
    {
      "matches": [
        "*://*.labflow.com/*",
        "*://*.labflow.ca/*"
      ],
      "js": ["content.js"]
    }
  ],
  "permissions": [],
  "background": {
    "service_worker": "background.js"
  }
}